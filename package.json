{
	"name": "uyghurscript",
	"displayName": "uyghurscript",
	"publisher": "kompasim",
	"description": "uyghur script language extension ...",
	"icon": "others/icon.png",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.69.0"
	},
    "keywords": [
		"uyghurscript",
		"uyghurche",
		"uyghur",
		"uighurche",
		"uighur"
	],
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kompasim/uyghurscript-vscode-extension.git"
	},
	"homepage": "https://github.com/kompasim/uyghurscript-vscode-extension",
	"bugs": {
		"url": "https://github.com/kompasim/uyghurscript-vscode-extension/issues",
		"email": "kompasim@163.com"
	},
	"contributes": {
		"languages": [
			{
				"id": "ug",
				"aliases": [
					"uyghurscript",
					"ug"
				],
				"extensions": [
					".ug",
					".uy"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ug",
				"scopeName": "uyghurscript.ug",
				"path": "./syntaxes/ug.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "ug",
				"path": "./language-snippets.json"
			}
		]
	},
