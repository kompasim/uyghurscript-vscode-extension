{
    "name": "uyghurscript",
    "displayName": "uyghurscript",
    "description": "uyghur script language extension ...",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "ug",
            "aliases": ["uyghurscript", "ug"],
            "extensions": [".ug","uy",".ugs",".uys"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ug",
            "scopeName": "uyghurscript.ug",
            "path": "./syntaxes/ug.tmLanguage.json"
        }],
        "snippets": [{
            "language": "ug",
            "path": "./language-snippets.json"
        }]
    }
}